<script type="text/javascript">
var len = 100;
var p = document.getElementById('truncateContent');
if (p) {
  var trunc = p.innerHTML;
  if (trunc.length > len) {
    /* Truncate the content of the P, then go back to the end of the
       previous word to ensure that we don't truncate in the middle of
       a word */
       var arry = trunc.split( /<PERSON_PROFILE_TEXT>/i);
       if(arry.length == 2){
       trunc = arry[1];
       }
		var arry2 = trunc.split( '<\/PERSON_PROFILE_TEXT>');
 		if(arry2.length == 2){
        trunc = arry2[0];
         }
         trunc = trunc.replace(/<+\S+>/ig, "");   
         trunc = trunc.replace(/<+[^>]+>?/ig,"");  
    	 trunc = trunc.substring(0, len);
         trunc = trunc.replace(/\w+$/, '');
    var isAlphaNumeric = false;
    while(isAlphaNumeric == false){  
    var testString = trunc.substring(trunc.length - 3 ,trunc.length);    
    var isAlphaNumeric = alphanumeric(testString);   
    if(!isAlphaNumeric ){
     trunc = trunc.substring(0, trunc.length -1);   
    }
    }
    /* Add an ellipses to the end and make it a link that expands
       the paragraph back to its original size */
    trunc += ' | '+'<a href="/teams/coaches?coaType=head&team=ATL"> '+
      'More<\/a>';
    p.innerHTML = trunc;
  }
}

function alphanumeric(alphane)
{
	var numaric = alphane;
	for(var j=0; j<numaric.length; j++)
		{
		  var alphaa = numaric.charAt(j);
		  var hh = alphaa.charCodeAt(0);
		  if((hh > 47 && hh<59) || (hh > 64 && hh<91) || (hh > 96 && hh<123))
		  {
		  }
		else	{
			 return false;
		  }
		}
 return true;
}
</script>


