<script>

nfl.use('node','transition',function(Y){
	Y.on('domready',function(){

		var miniNav = [
			'<div class="events-mini-nav">',
				'<div class="events-mini-wrapper">',
					'<ul>',
						'<li><a href="/draft/2011" class="draft-2011">Draft \'11</a></li>',
						'<li><a href="/trainingcamp" class="training-camp">Training Camp</a></li>',
						'<li><a href="/kickoff" class="kickoff">Kickoff</a></li>',
						'<li><a href="/international" class="international">International</a></li>',
						'<li><a href="/thanksgiving" class="thanksgiving">Thanksgiving</a></li>',
						'<li><a href="/seniorbowl" class="senior-bowl">Senior Bowl</a></li>',
						'<li><a href="/probowl" class="pro-bowl">Pro Bowl</a></li>',
						'<li><a href="/superbowl/46" class="super-bowl-46">Super Bowl</a></li>',
						'<li><a href="/combine" class="combine">Combine</a></li>',
						'<li><a href="/draft/2012" class="draft-2012">Draft \'12</a></li>',
					'</ul>',
				'</div>',
			'</div>'
		].join('');
		miniNav = Y.Node.create(miniNav);
		miniNav.all('a').each(function(a){
			if(a.hasClass('draft-2011')){
				a.addClass('selected');
			}			
		});
		miniNav.setStyle('height','0px');
		Y.one('body').prepend(miniNav);

		var pageShowHide = function (openHeight,closedHeight){
			this.above = null;
			this.openHeight = openHeight;
			this.closedHeight = closedHeight;
			this.checkPos = function(){
				var navPos = Y.one('.events-nav-general');			
				var navBottom = navPos.get('region').bottom;
				var viewPortTop = navPos.get('viewportRegion').top; 			

				if( (navBottom < viewPortTop) && ((this.above == null) || (this.above == false))){
					Y.one('.events-mini-nav').transition({easing:'ease-out',duration:.4,height:this.openHeight});					
					this.above = true;
				}else if( (navBottom > viewPortTop) && ((this.above == null) || (this.above == true)) ){
					this.above = false;					
					Y.one('.events-mini-nav').transition({easing:'ease-out',duration:.4,height:this.closedHeight});
					
				}
			}
		}
    
		var miniNavReveal = new pageShowHide('37px','0px');
		miniNavReveal.checkPos();

		Y.on('scroll',Y.bind(miniNavReveal.checkPos,miniNavReveal),Y.config.win);

		/************
		TODO: DELETE ONCE WE WIRE IN CAROUSEL
		************/
		Y.one('.events-nav-next').on('click',function(e){Y.log(e);e.preventDefault();},this)
		Y.one('.events-nav-back').on('click',function(e){Y.log(e);e.preventDefault();},this)
	})
})
</script>
