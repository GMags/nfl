	<script type="text/javascript">
/***************************************/
/* this overrides a setting in nfl.ui.behaviors.js.  i changed it here because it cant be a global change right now. */
nfl.ui.behaviors.thumbs.tabs.rowClasses			= {on:'tbdy-sorted',off:'tbdy1'};
/***************************************/
function teamStatsNav(teamAbbr){
	if(teamAbbr != 0) {
		window.location.pathname = "/teams/statistics?team=" + teamAbbr;
	}
}
function goToCategoryStats(url) {
	if(url != '') {
		window.location.href=url;
	}
}
function removeAllChildren(element) {
	while (element.firstChild) {
		element.removeChild(element.firstChild);
	}
}
function setSeasonTypes(element, evalString) {
	removeAllChildren(element);
	var opts = eval(evalString);
	
	for (var i in opts){
	    var o = document.createElement('option');
		o.innerHTML = "";
		o.text = opts[i];
		o.value = i;
		element.appendChild(o);	
	} 
}
function changeSeasonType() {
 	new Ajax.Request('/ajax/playerstat', { 
		 method: 'get', 
		 parameters: { seasonId: $("seasonId").value }, 
		 onSuccess: function(transport) {
			setSeasonTypes($('seasonTypeSelect'), '({' + transport.responseText + '})');	
  		 }
	 });
}
function nocover(cover) {
	cover.src = 'http://img.static.nfl.com/static/site/3.5/img/teams/DEFAULT_TEAM/Default_Team_80_90.gif';
}
	</script>
