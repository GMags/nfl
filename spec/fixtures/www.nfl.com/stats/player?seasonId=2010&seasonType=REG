	function onPlayerStatsLandingLoad(){
			var headshots = document.getElementsByName('theImages1');
			var headshotsArray = new Array();
			for(var j=0; j<headshots.length; j++){
				headshotsArray[j] = headshots[j].value;
			}
			theImages1 = nfl.util.images.preloader.loadImages2(headshotsArray);
			
			headshots = document.getElementsByName('theImages2');
			headshotsArray = new Array();
			for(var j=0; j<headshots.length; j++){
				headshotsArray[j] = headshots[j].value;
			}
			theImages2 = preloadImages2(headshotsArray);
			
			headshots = document.getElementsByName('theImages3');
			headshotsArray = new Array();
			for(var j=0; j<headshots.length; j++){
				headshotsArray[j] = headshots[j].value;
			}
			theImages3 = preloadImages2(headshotsArray);
			
			headshots = document.getElementsByName('theImages4');
			headshotsArray = new Array();
			for(var j=0; j<headshots.length; j++){
				headshotsArray[j] = headshots[j].value;
			}
			theImages4 = preloadImages2(headshotsArray);
			
